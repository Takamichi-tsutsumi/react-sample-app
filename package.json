{
  "name": "react-sample-app",
  "version": "0.1.0",
  "description": "React Sample App",
  "main": "dist/client.js",
  "scripts": {
    "start": "node ./scripts/start.js",
    "dev:server": "node dist/server.js",
    "bench": "autocannon http://localhost:2233",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tkmttm/react-sample-app.git"
  },
  "keywords": [],
  "author": "ttsutsumi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tkmttm/react-sample-app/issues"
  },
  "homepage": "https://github.com/tkmttm/react-sample-app#readme",
  "lint-staged": {
    "*.{ts, tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.js": [
      "npx prettier --no-semi --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-config": "^1.1.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack-env": "^1.13.6",
    "assets-webpack-plugin": "^3.9.7",
    "autocannon": "^3.1.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "opn": "^5.4.0",
    "prettier": "^1.15.2",
    "react-dev-utils": "^6.1.1",
    "react-test-renderer": "^16.6.3",
    "start-server-webpack-plugin": "^2.2.5",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "express": "^4.16.4",
    "loadable-components": "^2.2.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
